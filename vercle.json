{
  "version": 2,
  "builds": [
    {
      "src": "api/mega.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/pair.html"
    },
    {
      "src": "/pair.js",
      "dest": "/pair.js"
    },
    {
      "src": "/api/mega",
      "dest": "/api/mega.js"
    }
  ]
}
